            $('.js-files').change(function () {
                $('.js-file-block').show();
                var fileContainer = $('.js-file-container');
                var fileTemplate = $('.js-file-template');

                // remove files that were attached before
                fileContainer.html('');

                var files = $(this)[0].files;
                $.each(files, function (k, file) {
                    // browser returns fake path, get only filename
                    var filename = file.name.split(/(\\|\/)/g).pop();

                    // insert into template filename
                    var html = $('<div>', {html:fileTemplate.html()});
                    html.find('.js-filename').text(filename);

                    fileContainer.append(html.html());
                });
            });

            $('body').on('click', '.js-remove', function () {
                $('.js-file-container').html('');

                // reset file input
                var files = $('.js-files');
                $.each(files, function (k, file) {
                    $(file).val('');
                });

            });
        });
